# Amber Electric API Configuration
# Get your API key from: https://app.amber.com.au/developers
AMBER_API_KEY=your_amber_electric_api_key_here

# PostgreSQL Database Configuration
# These match the docker-compose.yml defaults
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=amber_home
POSTGRES_USER=amber_user
POSTGRES_PASSWORD=amber_password

# Streamlit Dashboard Configuration
STREAMLIT_PORT=8501
STREAMLIT_HOST=0.0.0.0

# Data Collection Configuration
# The API has a limit of 7 days per request and 50 calls per 5 minutes
DEFAULT_COLLECTION_DAYS=7
RENEWABLE_STATE=vic